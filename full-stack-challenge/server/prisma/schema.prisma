
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_CONNECTION")
}

model Token {
  id        String   @id @default(cuid())
  name      String
  imageURL  String
  address   String
  decimals  Int
  ticker    String
}

model Logger {
  id        Int      @id @default(autoincrement())
  method    String
  endpoint  String
  data      Json?
  createdAt DateTime @default(now())
}